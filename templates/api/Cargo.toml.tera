[package]
name = "google-{{ api.name }}{{ api.version }}"
version = "0.1.0"  # Fixed version number
edition = "2021"
description = "{{ api.description }}"
license = "MIT"

[dependencies]
serde = { version = "1.0", features = ["derive"] }
serde_json = "1.0"
hyper = { version = "0.14", features = ["full"] }
hyper-rustls = "0.24"
tokio = { version = "1.0", features = ["full"] }
anyhow = "1.0"
yup-oauth2 = "8.1"